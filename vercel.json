{
  "version": 2,
  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python",
      "config": {
        "runtime" : "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET","Post"]
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "./index.py",
    "VERCEL": "true"
  }
}
